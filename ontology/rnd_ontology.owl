<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/rnd_ontology"
         xmlns="http://example.org/rnd_ontology#">

<owl:Ontology rdf:about="http://example.org/rnd_ontology"/>

<owl:ObjectProperty rdf:about="#hasLeadResearcher">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#Researcher"/>
  <owl:inverseOf rdf:resource="#isLeadResearcherOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasParticipant">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#Researcher"/>
  <owl:inverseOf rdf:resource="#participatesIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasFundingAgency">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#Organization"/>
  <owl:inverseOf rdf:resource="#funds"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasExecutingOrg">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#Organization"/>
  <owl:inverseOf rdf:resource="#executes"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#producesOutput">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#ResearchOutput"/>
  <owl:inverseOf rdf:resource="#isProducedBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#usesTechnology">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#Technology"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#developsTechnology">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:range rdf:resource="#Technology"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasKeyword">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:domain rdf:resource="#Technology"/>
  <rdfs:domain rdf:resource="#ResearchOutput"/>
  <rdfs:range rdf:resource="#Keyword"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#belongsToField">
  <rdfs:domain rdf:resource="#ResearchProject"/>
  <rdfs:domain rdf:resource="#Technology"/>
  <rdfs:domain rdf:resource="#ResearchOutput"/>
  <rdfs:range rdf:resource="#ResearchField"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#worksAt">
  <rdfs:domain rdf:resource="#Researcher"/>
  <rdfs:range rdf:resource="#Organization"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#collaboratesWith">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdfs:domain rdf:resource="#Researcher"/>
  <rdfs:range rdf:resource="#Researcher"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#authorsOutput">
  <rdfs:domain rdf:resource="#Researcher"/>
  <rdfs:range rdf:resource="#ResearchOutput"/>
  <owl:inverseOf rdf:resource="#isAuthoredBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#relatedTo">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdfs:domain rdf:resource="#Technology"/>
  <rdfs:range rdf:resource="#Technology"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#basedOn">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="#Technology"/>
  <rdfs:range rdf:resource="#Technology"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isLeadResearcherOf">
  <owl:inverseOf rdf:resource="#hasLeadResearcher"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#participatesIn">
  <owl:inverseOf rdf:resource="#hasParticipant"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#funds">
  <owl:inverseOf rdf:resource="#hasFundingAgency"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#executes">
  <owl:inverseOf rdf:resource="#hasExecutingOrg"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isProducedBy">
  <owl:inverseOf rdf:resource="#producesOutput"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAuthoredBy">
  <owl:inverseOf rdf:resource="#authorsOutput"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasName">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#Entity"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasDescription">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#Entity"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasStartDate">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#ResearchProject"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasEndDate">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#ResearchProject"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasBudget">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
  <rdfs:domain rdf:resource="#ResearchProject"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasStatus">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#ResearchProject"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasYear">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  <rdfs:domain rdf:resource="#ResearchOutput"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasAbstract">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#ResearchOutput"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasAffiliation">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#Researcher"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasExpertise">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:domain rdf:resource="#Researcher"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Entity">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#ResearchProject">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">R&amp;D 연구과제를 나타내는 클래스</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Researcher">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">연구를 수행하는 개인</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Organization">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">연구기관, 대학, 기업 등</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Technology">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">연구에서 개발되거나 사용되는 기술</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#ResearchOutput">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">논문, 특허, 보고서 등 연구 결과물</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Keyword">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">연구 분야나 주제를 나타내는 키워드</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#ResearchField">
  <rdfs:subClassOf rdf:resource="#Entity"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">연구 분야 분류</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Paper">
  <rdfs:subClassOf rdf:resource="#ResearchOutput"/>
</owl:Class>

<owl:Class rdf:about="#Patent">
  <rdfs:subClassOf rdf:resource="#ResearchOutput"/>
</owl:Class>

<owl:Class rdf:about="#Report">
  <rdfs:subClassOf rdf:resource="#ResearchOutput"/>
</owl:Class>

<owl:Class rdf:about="#Software">
  <rdfs:subClassOf rdf:resource="#ResearchOutput"/>
</owl:Class>

<owl:Class rdf:about="#University">
  <rdfs:subClassOf rdf:resource="#Organization"/>
</owl:Class>

<owl:Class rdf:about="#ResearchInstitute">
  <rdfs:subClassOf rdf:resource="#Organization"/>
</owl:Class>

<owl:Class rdf:about="#Company">
  <rdfs:subClassOf rdf:resource="#Organization"/>
</owl:Class>

<owl:Class rdf:about="#GovernmentAgency">
  <rdfs:subClassOf rdf:resource="#Organization"/>
</owl:Class>


</rdf:RDF>
